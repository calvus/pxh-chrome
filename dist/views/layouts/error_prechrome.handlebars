  <head>
    <title>Predix: Error</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" type="image/png" href="{{baseUrl}}/bower_components/pxh-chrome/dist/favicon.ico">
    <link rel="stylesheet" href="{{baseUrl}}/bower_components/pxh-chrome/dist/css/pxh-prechrome.min.css">
  </head>
  <body>
    <div class="pxh-error pxh-error@md pxh-error@lg">
      <div class="pxh-error__icon">
        <i class="fa fa-exclamation-circle"></i>
      </div>
      <h1 class="pxh-error__title">{{title}}</h1>
      <p class="pxh-error__description">{{{description}}}</p>
    </div>
  </body>
</html>
