<!doctype html>
<html lang="en">
  <!-- ui-app-hub build version: ##BUILDVERSION##  (DO NOT DELETE THIS BLOCK) -->
  <head>
    <title>{{appname}}: Error</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" type="image/png" href="{{baseUrl}}/bower_components/pxh-chrome/dist/favicon.ico">
    <link rel="stylesheet" href="{{baseUrl}}/bower_components/pxh-chrome/dist/css/pxh-chrome.min.css">
  </head>
  <body class="pxh-chromeless">
    <script>
      // put the nav object on the window, so it's available in javascript
      window.nav = {{{navStr}}};
    </script>
    <div class="pxh-wrapper">
      <div class="pxh-view-header pxh-view-header--wide@md pxh-view-header--narrow@lg">
      </div>
      <div class="pxh-view pxh-view--wide@md pxh-view--narrow@lg pxh-view--white" id="js-view">
        <div class="pxh-error pxh-error@md pxh-error@lg">
          <div class="pxh-error__icon">
            <i class="fa fa-exclamation-circle"></i>
          </div>
          <h1 class="pxh-error__title">{{title}}</h1>
          <p class="pxh-error__description">{{{description}}}</p>
        </div>
      </div>
    </div>
  </body>
</html>
